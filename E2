#include <iostream>

// Function prototype
int addNumbers(int num1, int num2);

int main() {
    int a, b;

    std::cout << "Enter two numbers to add: ";
    std::cin >> a >> b;

    int sum = addNumbers(a, b);

    std::cout << "Sum of " << a << " and " << b << " is: " << sum << std::endl;

    return 0;
}

// Function definition
int addNumbers(int num1, int num2) {
    return num1 + num2;
}
